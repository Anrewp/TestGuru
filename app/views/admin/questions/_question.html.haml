%tr
  %td= question.id
  %td= question.body
  %td= link_to 'Show', admin_question_path(question), method: :get
  %td= link_to 'Edit', edit_admin_question_path(question)
  %td= link_to 'Delete', admin_question_path(question), method: :delete, data: {confirm: 'Are you sure you want to delete this question?'}
