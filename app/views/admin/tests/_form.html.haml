= form_with model: [:admin, @test], local: true do |f|
  %p
    = f.label :title
    = f.text_field :title, id: :test_title
  %p
    = f.label :level
    = f.text_field :level, id: :test_level
  %p
    = f.label :category_id
    = f.collection_select :category_id, Category.unscoped, :id, :title, prompt: true
  %p
    = f.hidden_field :author_id, value: current_user.id
  %p
    = f.submit
